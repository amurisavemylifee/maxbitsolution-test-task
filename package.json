{
  "name": "maxbitsolution-test-task",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "cd backend && npm run start",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "generate:api": "(cd backend && node --input-type=module -e 'import { swaggerSpec } from \"./src/swagger.js\"; console.log(JSON.stringify(swaggerSpec, null, 2));' > swagger.json) && openapi --input ./backend/swagger.json --output ./src/shared/api/generated --useOptions --client axios --indent 2 && rm ./backend/swagger.json"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "pinia": "^3.0.4",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.0.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "@vitest/coverage-v8": "^4.0.17",
    "jsdom": "^27.4.0",
    "concurrently": "^9.2.1",
    "openapi-typescript-codegen": "^0.30.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17",
    "vue-tsc": "^3.2.2"
  }
}
